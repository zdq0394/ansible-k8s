apiVersion: v1
kind: Config
preferences: {}
clusters:
- cluster:
    certificate-authority: /etc/kubernetes/ssl/ca.crt
    server: {{ apiserver_endpoint_https }}
  name: myk8s
users:
- user:
    token: {{ token_auth_file_token }}
  name: kubelet-bootstrap
contexts:
- context:
    cluster: myk8s
    user: kubelet-bootstrap
  name: my-context
current-context: my-context
